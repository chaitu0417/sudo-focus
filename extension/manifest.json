{
  "manifest_version": 3,
  "name": "Sudo Focus",
  "version": "1.0",
  "description": "Blocks sites until you code.",
  "permissions": ["tabs", "storage"],
  "host_permissions": [
    "http://localhost:3000/*",
    "*://*.instagram.com/*",
    "*://*.facebook.com/*",
    "*://leetcode.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://leetcode.com/problems/*"],
      "js": ["content.js"]
    }
  ]
}
